# Supabase Configuration
# URL of your Supabase project (e.g., https://xxxx.supabase.co)
VITE_SUPABASE_URL=
# Public/anon key for Supabase client access (never use service role in frontend)
VITE_SUPABASE_ANON_KEY=

# API Configuration
# API Base URL for frontend to call backend (e.g., https://api.yourdomain.com)
VITE_API_URL=

# Stripe Configuration (frontend)
# Publishable key used by Stripe.js
VITE_STRIPE_PUBLISHABLE_KEY=

# Sales contact email used in Pricing page
VITE_SALES_EMAIL=

